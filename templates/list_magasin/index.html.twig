  

            
{% extends "base_admin.html.twig" %}
{% block content %}
    
           
            <div class="main-content">

                <div class="page-content">
                    <div class="container-fluid">

                        
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                    <h4 class="mb-sm-0 font-size-18">Magasins</h4>

                                </div>
                            </div>
                        </div>
                        

                        <div class="row">
                            <div class="col-12">
                                <div class="card">
                                    
                                    <div class="card-body">
        
                                        <table id="datatable" class="table table-bordered dt-responsive  nowrap w-100">
                                            <thead>
                                            <tr>
                                                <th>nom</th>
                                                <th>adresse-email</th>
                                                <th>pays</th>
                                                <th>gouvernorat</th>
                                                <th>ville</th>
                                                <th>rue</th>
                                                 <th>action</th>

                                            </tr>
                                            </thead>
        
        
                                            <tbody>
                                            {% for m in magasins %}
                                            <tr>
                                                <td>{{m.nom}}</td>
                                                
                                                <td>{{m.email}}</td>
                                                <td>{{m.pays}}</td>
                                                <td>{{m.gouvernorat}}</td>
                                                <td>{{m.ville}}</td>
                                                <td>{{m.rue}}</td>
                                                <td><a href="{{ path('bloquer_magasin', {id:m.id}) }}" class="btn btn-outline-warning waves-effect waves-light">bloquer</a>
                                                 <a href="{{ path('delete_magasin', {id:m.id}) }}" class="btn btn-outline-danger waves-effect waves-light">supprimer</a>
                                                </td>

                                                
                                            </tr>
                                            {% endfor %}
                                 
                                            </tbody>
                                        </table>
        
                                    </div>
                                </div>
                            </div> 
                        </div> 
        
                    </div> 
                </div>
                
              
                
            </div>
            
{% endblock %}         
       

